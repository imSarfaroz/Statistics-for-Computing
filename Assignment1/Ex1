# Ex 1
import pylab

# list of int
dataSet = [1, 1, 1, 5, 5, 3, 1, 3, 3, 1 ,4, 4, 4, 2, 2, 2, 2]

# Function to count the frequences
def freqsOfNums(dataSet):
    
    counter = {} #empty dic
    for i in dataSet:
        #counting the freq of each number
        counter[i] = dataSet.count(i)
    
    # printing the results and displaying in bar chart
    for key, value in counter.items():
        print("%d -> %d"%(key, value))
        pylab.bar(key, value)
        pylab.xlabel('Number of Siblings')
        pylab.ylabel('Frequency')
        
# main
freqsOfNums(dataSet
